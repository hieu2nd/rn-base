{
  "name": "TemplateAd",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 8'",
    "start": "react-native start",
    "test": "jest",
    "i": "--ignore-engines",
    "link_assets": "npx react-native-asset",
    "rename": "npx react-native-rename ",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "reverse": "adb reverse tcp:9090 tcp:9090; adb reverse tcp:19001 tcp:19001;",
    "pod": "cd ios && pod install && cd ..",
    "createAppCenterAndroid": "appcenter apps create -d TemplateAd -o Android -p React-Native",
    "createAppCenterIOS": "appcenter apps create -d TemplateAd -o iOS -p React-Native",
    "codePushStagging": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd-1 -d Staging",
    "codePushRelease": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd-1 -d Production",
    "codePushReleaseAndroid": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd -d Production",
    "codePushStagingAndroid": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd -d Staging",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-firebase/app": "^15.6.0",
    "@react-native-firebase/auth": "^15.6.0",
    "@react-native-google-signin/google-signin": "^8.0.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/material-bottom-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.3.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@shopify/flash-list": "^1.2.2",
    "@shopify/restyle": "^2.1.0",
    "add": "^2.0.6",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "axios": "^0.27.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "formik": "^2.2.9",
    "i18next": "^21.9.2",
    "i18next-http-backend": "^1.4.2",
    "lodash": "^4.17.21",
    "react": "18.1.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.6",
    "react-native-bootsplash": "^4.4.0",
    "react-native-code-push": "^7.0.5",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.2.0",
    "react-native-fast-image": "^8.6.1",
    "react-native-fbsdk-next": "^10.1.0",
    "react-native-flash-message": "^0.3.1",
    "react-native-gesture-handler": "^2.6.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^13.2.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-text-ticker": "^1.14.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.2",
    "reactotron-react-native": "^5.0.2",
    "reactotron-redux": "^3.1.3",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "yarn": "^1.22.19",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.0.3",
    "@types/react-native": "^0.70.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^29.0.3",
    "eslint": "^8.23.1",
    "jest": "^29.0.3",
    "metro-react-native-babel-preset": "^0.72.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier src --write",
      "eslint src --fix",
      "git add"
    ]
  }
}
